<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Usuario.
    -->
    <changeSet id="20230208010147-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="funcionalidad_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__funcionalidad_id"
                                 referencedColumnNames="id"
                                 referencedTableName="funcionalidad"/>

        <addForeignKeyConstraint baseColumnNames="proyecto_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__proyecto_id"
                                 referencedColumnNames="id"
                                 referencedTableName="proyecto"/>

        <addForeignKeyConstraint baseColumnNames="rol_id"
                                 baseTableName="usuario"
                                 constraintName="fk_usuario__rol_id"
                                 referencedColumnNames="id"
                                 referencedTableName="rol"/>
    </changeSet>
</databaseChangeLog>
